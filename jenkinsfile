pipeline {
    agent any
     
    stages {
      stage('checkout') {
           steps {
             
                git 'https://github.com/sruthiyelti/spring-petclinic.git'
          }
        }
        stage('code'){
            steps {
                withSonarQubeEnv(credentialsId: 'sonartoken') {
               sh 'mvn package sonar:sonar'  
   } 
        
   }
       }
           }
}
